{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "jsx": "preserve",
    "strictFunctionTypes": true,
    "sourceMap": true,
    "lib": ["DOM", "ESNext"],
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
